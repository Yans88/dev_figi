CREATE ALGORITHM=UNDEFINED VIEW `facility_book_view` AS select `ce`.`id_book` AS `id_book`,`ce`.`id_user` AS `id_user`,`ce`.`id_facility` AS `id_facility`,`ce`.`dt_start` AS `dt_start`,`ce`.`dt_end` AS `dt_end`,`ce`.`duration` AS `duration`,`ce`.`repetition` AS `repetition`,`ce`.`interval` AS `interval`,`ce`.`dt_last` AS `dt_last`,`ce`.`wd_start` AS `wd_start`,`ce`.`purpose` AS `purpose`,`ce`.`remark` AS `remark`,`ce`.`status` AS `status`,`ce`.`fullday` AS `fullday`,`ce`.`book_date` AS `book_date`,`u`.`user_name` AS `user_name`,`u`.`full_name` AS `full_name`,`u`.`user_email` AS `user_email`,`u`.`contact_no` AS `contact_no`,`u`.`id_group` AS `id_group`,`u`.`id_department` AS `id_department`,`u`.`nric` AS `nric`,`l`.`location_name` AS `location_name` from (((`facility_book` `ce` left join `facility` `f` on((`ce`.`id_facility` = `f`.`id_facility`))) left join `location` `l` on((`f`.`id_location` = `l`.`id_location`))) left join `user` `u` on((`u`.`id_user` = `ce`.`id_user`)));


CREATE ALGORITHM=UNDEFINED  VIEW `calendar_view` AS select `ce`.`id_event` AS `id_event`,`ce`.`id_user` AS `id_user`,`ce`.`id_location` AS `id_location`,`ce`.`dt_start` AS `dt_start`,`ce`.`dt_end` AS `dt_end`,`ce`.`duration` AS `duration`,`ce`.`repetition` AS `repetition`,`ce`.`interval` AS `interval`,`ce`.`dt_last` AS `dt_last`,`ce`.`wd_start` AS `wd_start`,`ce`.`title` AS `title`,`ce`.`description` AS `description`,`ce`.`status` AS `status`,`ce`.`fullday` AS `fullday`,`ce`.`cdate` AS `cdate`,`ce`.`mdate` AS `mdate`,`ce`.`type` AS `type`,`u`.`user_name` AS `user_name`,`u`.`full_name` AS `full_name`,`u`.`user_email` AS `user_email`,`u`.`contact_no` AS `contact_no`,`u`.`id_group` AS `id_group`,`u`.`id_department` AS `id_department`,`u`.`nric` AS `nric`,`l`.`location_name` AS `location_name` from ((`calendar_events` `ce` left join `location` `l` on((`ce`.`id_location` = `l`.`id_location`))) left join `user` `u` on((`u`.`id_user` = `ce`.`id_user`)));

